{
    "name": "@konoui/mjimage",
    "version": "0.0.30",
    "description": "Generates Mahjong tiles in SVG format.",
    "author": "konoui",
    "license": "MIT",
    "repository": "github.com/konoui/mjimage",
    "sideEffects": false,
    "keywords": [
        "mahjong"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "test": "jest",
        "cmd": "ts-node ./src/cmd/index.ts",
        "tsc": "tsc --declaration --outDir ./dist/mjs",
        "prepublishOnly": "rm -rf ./dist && npm run tsc"
    },
    "types": "dist/mjs/index.d.ts",
    "files": [
        "dist/",
        "!**/__tests__/"
    ],
    "exports": {
        ".": "./dist/mjs/index.js",
        "./core": "./dist/mjs/lib/core/index.js",
        "./image": "./dist/mjs/lib/image/index.js",
        "./table": "./dist/mjs/lib/table/index.js",
        "./calculator": "./dist/mjs/lib/calculator/index.js"
    },
    "targets": {
        "module": {}
    },
    "devDependencies": {
        "@types/jest": "^29.5.5",
        "@types/js-yaml": "^4.0.9",
        "@types/svgdom": "^0.1.0",
        "assert": "^2.1.0",
        "buffer": "^6.0.3",
        "process": "^0.11.10",
        "svg-sprite": "^2.0.2",
        "svgdom": "0.1.14",
        "svgo": "^3.0.2",
        "ts-jest": "^29.1.1",
        "typescript": "^5.5.4"
    },
    "dependencies": {
        "@svgdotjs/svg.js": "^3.2.4",
        "eventmit": "^2.0.4",
        "js-yaml": "^4.1.0",
        "valibot": "^0.40.0",
        "xstate": "^5.7.0"
    }
}
