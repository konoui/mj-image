{
    "name": "mjimage",
    "version": "0.1.0",
    "description": "",
    "scripts": {
        "start": "parcel",
        "build": "parcel build",
        "dev": "parcel src/index.html",
        "test": "jest",
        "svg-sprite": "svg-sprite -C svg-sprite.config.json -shape-transform-svgo svgo.json static/svg/*.svg"
    },
    "targets": {
        "global": {
            "source": "src/global.ts",
            "context": "browser",
            "includeNodeModules": true,
            "isLibrary": false,
            "optimize": true,
            "distDir": "./dist/global/",
            "engines": {
                "browsers": "> 0.5%, last 2 versions, not dead"
            }
        },
        "module": {
            "source": "src/index.ts",
            "includeNodeModules": true,
            "outputFormat": "esmodule",
            "engines": {
                "browsers": "> 0.5%, last 2 versions, not dead"
            },
            "optimize": true
        },
        "main": {
            "source": "src/index.ts",
            "outputFormat": "commonjs",
            "optimize": true
        },
        "webapp": {
            "source": "src/index.html",
            "distDir": "./dist/webapp/"
        }
    },
    "alias": {},
    "staticFiles": {
        "staticPath": "static/svg",
        "staticOutPath": "svg"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@parcel/transformer-inline-string": "^2.9.3",
        "@parcel/transformer-typescript-tsc": "^2.9.3",
        "@parcel/validator-typescript": "^2.9.3",
        "@types/jest": "^29.5.5",
        "@types/js-yaml": "^4.0.9",
        "@types/svgdom": "^0.1.0",
        "assert": "^2.1.0",
        "buffer": "^6.0.3",
        "jest": "^29.7.0",
        "parcel": "^2.9.3",
        "parcel-reporter-static-files-copy": "^1.5.2",
        "process": "^0.11.10",
        "svg-sprite": "^2.0.2",
        "svgdom": "^0.1.16",
        "svgo": "^3.0.2",
        "ts-jest": "^29.1.1",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "@svgdotjs/svg.js": "^3.2.0",
        "js-yaml": "^4.1.0",
        "zod": "^3.22.4"
    }
}
